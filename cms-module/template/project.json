{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "{{data.settings.flow}}/{{data.path}}/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["{{data.settings.flow}}/{{data.path}}/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/{{data.settings.flow}}/{{data.path}}"],
      "options": {
        "jestConfig": "{{data.settings.flow}}/{{data.path}}/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs {{data.path}} {args.catalogPath}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    }
  }
}
