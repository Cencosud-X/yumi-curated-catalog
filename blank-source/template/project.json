{
  "root": "packages/{{data.path}}",
  "sourceRoot": "packages/{{data.path}}/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/{{data.path}}",
        "tsConfig": "packages/{{data.path}}/tsconfig.lib.json",
        "project": "packages/{{data.path}}/package.json",
        "entryFile": "packages/{{data.path}}/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/{{data.path}}/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/{{data.path}}/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/{{data.path}}"],
      "options": {
        "jestConfig": "packages/{{data.path}}/jest.config.js",
        "passWithNoTests": true
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs {{data.path}} {args.release}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    }
  }
}
