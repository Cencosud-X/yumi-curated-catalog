{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "{{data.settings.PORTA}}/{{data.path}}/src",
  "targets": {
    "build": {
      "dependsOn": [
        {
          "target": "secrets",
          "projects": "self"
        }
      ],
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "release",
      "options": {
        "outputPath": "dist/{{data.settings.PORTA}}/{{data.path}}",
        "compiler": "babel",
        "index": "{{data.settings.PORTA}}/{{data.path}}/src/index.html",
        "baseHref": "/",
        "main": "{{data.settings.PORTA}}/{{data.path}}/src/main.tsx",
        "polyfills": "{{data.settings.PORTA}}/{{data.path}}/src/polyfills.ts",
        "tsConfig": "{{data.settings.PORTA}}/{{data.path}}/tsconfig.app.json",
        "assets": ["{{data.settings.PORTA}}/{{data.path}}/src/assets", "{{data.settings.PORTA}}/{{data.path}}/src/config"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "{{data.settings.PORTA}}/{{data.path}}/webpack.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "release": {
          "optimization": true,
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": true
        }
      }
    },
    "serve": {
      "dependsOn": [
        {
          "target": "secrets",
          "projects": "self"
        }
      ],
      "executor": "@nrwl/web:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "{{data.path}}:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "{{data.path}}:build:development"
        },
        "release": {
          "buildTarget": "{{data.path}}:build:release",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["{{data.settings.PORTA}}/{{data.path}}/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/{{data.settings.PORTA}}/{{data.path}}"],
      "options": {
        "jestConfig": "{{data.settings.PORTA}}/{{data.path}}/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "secrets": {
      "executor": "nx:run-commands",
      "options": {
        "command": "seki secrets generate -p={{data.path}}",
        "cwd": "."
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs {{data.path}} {args.catalogPath}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    }
  },
  "tags": ["REQUIRED:GOLDEN"]
}
