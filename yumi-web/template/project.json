{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "{{data.group_folder}}/{{data.path}}/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/{{data.group_folder}}/{{data.path}}",
        "index": "{{data.group_folder}}/{{data.path}}/src/index.html",
        "baseHref": "/",
        "main": "{{data.group_folder}}/{{data.path}}/src/main.tsx",
        "polyfills": "{{data.group_folder}}/{{data.path}}/src/polyfills.ts",
        "tsConfig": "{{data.group_folder}}/{{data.path}}/tsconfig.app.json",
        "assets": ["{{data.group_folder}}/{{data.path}}/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "{{data.group_folder}}/{{data.path}}/src/environments/environment.ts",
              "with": "{{data.group_folder}}/{{data.path}}/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "{{data.path}}:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "{{data.path}}:build:development"
        },
        "production": {
          "buildTarget": "{{data.path}}:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["{{data.group_folder}}/{{data.path}}/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/{{data.group_folder}}/{{data.path}}"],
      "options": {
        "jestConfig": "{{data.group_folder}}/{{data.path}}/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
